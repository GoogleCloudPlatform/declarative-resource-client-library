{
  "name": "{{pool}}",
  "project": "{{project}}",
  "location": "us-west1",
  "cluster": "{{ref:minimal.cluster.json:name}}",
  "version": "1.21-next",
  "autoscaling": {
    "minNodeCount": 1,
    "maxNodeCount": 5
  },
  "subnetId": "{{aws_subnet}}",
  "maxPodsConstraint": {
    "maxPodsPerNode": 110
  },
  "config": {
    "iamInstanceProfile": "{{byo_multicloud_prefix}}-1p-dev-nodepool"
  }
}
