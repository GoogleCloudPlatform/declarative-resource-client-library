{
  "name": "{{instance}}",
  "project": "{{project}}",
  "machineType": "zones/{{zone}}/machineTypes/e2-medium",
  "disks": [
    {
      "type": "PERSISTENT",
      "autoDelete": true,
      "boot": true,
      "initializeParams": {
        "sourceImage": "{{ref:basic.image.json:selfLink}}"
      }
    }
  ],
  "networkInterfaces": [
    {
      "subnetwork": "{{ref:independent.subnetwork.json:selfLink}}"
    },
    {
      "network": "global/networks/default",
      "accessConfigs": [
        {
          "type": "ONE_TO_ONE_NAT",
          "name": "External NAT"
        }
      ]
    }
  ],
  "zone": "{{zone}}"
}
